def factorial(n)
    if n == 1 do
        return 1;
    end 

    return n * factorial(n - 1);
end

def functions()
    def noArguments()
        return 123;
    end

    def withArguments(a b c)
        return a + b + c;
    end

    def fib(n)
        if n <= 1 do
            return n;
        end
        return fib(n - 1) + fib(n - 2);
    end

    def varArgsFunc(a b ..rest)
        return rest;
    end

    print(noArguments());
    print(withArguments(1, 2, 3));
    print(factorial(5));
    print(fib(10));
    print(varArgsFunc(1, 2, 3)); // TODO
end

